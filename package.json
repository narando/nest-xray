{
  "name": "@narando/nest-xray",
  "version": "1.4.2",
  "description": "Distributed tracing for Nestjs with AWS X-Ray",
  "author": "Julian TÃ¶lle <julian@narando.de>",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"lib/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand"
  },
  "dependencies": {
    "@narando/nest-axios-interceptor": "1.1.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.7.0 || ^7.0.0",
    "@nestjs/core": "^6.7.0 || ^7.0.0",
    "aws-xray-sdk": "^2.5.0 || ^3.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.3"
  },
  "devDependencies": {
    "@nestjs/cli": "7.6.0",
    "@nestjs/common": "8.0.7",
    "@nestjs/core": "7.6.18",
    "@nestjs/platform-express": "7.6.18",
    "@nestjs/schematics": "8.0.3",
    "@nestjs/testing": "7.6.18",
    "@types/express": "4.17.13",
    "@types/jest": "27.0.1",
    "@types/node": "14.17.12",
    "@types/supertest": "2.0.11",
    "aws-xray-sdk": "3.3.1",
    "jest": "27.0.6",
    "prettier": "2.2.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.6.7",
    "supertest": "4.0.2",
    "ts-jest": "27.0.5",
    "ts-loader": "9.2.5",
    "ts-node": "10.2.0",
    "tsconfig-paths": "3.10.1",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.3.0",
    "typescript": "4.0.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "lib",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/narando/nest-xray"
  }
}
