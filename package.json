{
  "name": "@narando/nest-xray",
  "version": "2.0.0",
  "description": "Distributed tracing for Nestjs with AWS X-Ray",
  "author": "Julian TÃ¶lle <julian@narando.de>",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"lib/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand"
  },
  "dependencies": {
    "@narando/nest-axios-interceptor": "2.0.0"
  },
  "peerDependencies": {
    "@nestjs/axios": "^0.0.5",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "aws-xray-sdk": "^2.5.0 || ^3.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0"
  },
  "devDependencies": {
    "@nestjs/axios": "0.0.5",
    "@nestjs/cli": "8.2.4",
    "@nestjs/common": "8.4.1",
    "@nestjs/core": "8.4.1",
    "@nestjs/platform-express": "8.4.1",
    "@nestjs/schematics": "8.0.10",
    "@nestjs/testing": "8.4.1",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.27",
    "@types/supertest": "2.0.12",
    "aws-xray-sdk": "3.3.4",
    "jest": "27.5.1",
    "prettier": "2.5.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.5",
    "supertest": "4.0.2",
    "ts-jest": "27.1.4",
    "ts-loader": "9.2.8",
    "ts-node": "10.4.0",
    "tsconfig-paths": "3.12.0",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.3.0",
    "typescript": "4.5.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "lib",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/narando/nest-xray"
  }
}
